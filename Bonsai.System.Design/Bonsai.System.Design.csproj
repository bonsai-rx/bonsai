<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <Title>Bonsai - System Design Library</Title>
    <Description>Bonsai Design Library containing editor classes for IO and other system configurations.</Description>
    <PackageTags>Bonsai Rx Reactive Extensions IO Resources Design</PackageTags>
    <UseWindowsForms>true</UseWindowsForms>
    <TargetFramework>net462</TargetFramework>
    <Version>2.7.0</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="OpenCV.Net" Version="3.4.0-preview3" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Bonsai.Design\Bonsai.Design.csproj" />
    <ProjectReference Include="..\Bonsai.System\Bonsai.System.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Design" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Resources\CollectionEditorControl.resx">
      <LogicalName>Bonsai.Resources.Design.CollectionEditorControl.resources</LogicalName>
    </EmbeddedResource>
  </ItemGroup>
</Project>