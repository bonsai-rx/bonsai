<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <Title>Bonsai32</Title>
    <Description>The x86 bootstrapper for the Bonsai environment.</Description>
    <PackageTags>Bonsai Rx Reactive Extensions</PackageTags>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateDocumentationFile>false</GenerateDocumentationFile>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <TargetFramework>net48</TargetFramework>
    <PlatformTarget>x86</PlatformTarget>
    <VersionPrefix>2.8.3</VersionPrefix>
    <OutputType>Exe</OutputType>
    <OutputPath>..\Bonsai\bin\$(Configuration)\</OutputPath>
    <ApplicationIcon>..\Bonsai.Editor\Bonsai.ico</ApplicationIcon>
    <ApplicationManifest>App.manifest</ApplicationManifest>
    <CopyLocalLockFileAssemblies>false</CopyLocalLockFileAssemblies>
    <DisableTransitiveProjectReferences>true</DisableTransitiveProjectReferences>
    <IsPackable>false</IsPackable>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Bonsai\Bonsai.csproj" />
    <PackageReference Include="System.Runtime.CompilerServices.Unsafe" Version="6.0.0" />
  </ItemGroup>
</Project>