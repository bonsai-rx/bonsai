<?xml version="1.0" encoding="utf-8"?>
<WorkflowBuilder Version="2.1.0">
  <Workflow xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:q3="clr-namespace:Bonsai.Audio;assembly=Bonsai.Audio" xmlns:q2="clr-namespace:Bonsai.Dsp;assembly=Bonsai.Dsp" xmlns:q1="clr-namespace:Bonsai.Arduino;assembly=Bonsai.Arduino" xmlns="https://horizongir.org/bonsai">
    <Nodes>
      <Expression xsi:type="Source">
        <PropertyMappings />
        <Generator xsi:type="q1:AnalogInput">
          <q1:PortName>COM4</q1:PortName>
          <q1:Pin>0</q1:Pin>
        </Generator>
      </Expression>
      <Expression xsi:type="Source">
        <PropertyMappings>
          <PropertyMapping name="Frequency" selector="Source1" />
          <PropertyMapping name="Waveform" selector="Source2" />
        </PropertyMappings>
        <Generator xsi:type="q2:FunctionGenerator">
          <q2:BufferLength>441</q2:BufferLength>
          <q2:Frequency>4</q2:Frequency>
          <q2:Waveform>Sine</q2:Waveform>
          <q2:PlaybackRate>100</q2:PlaybackRate>
        </Generator>
      </Expression>
      <Expression xsi:type="ExternalizedPropertyOfInt32FunctionGenerator">
        <MemberName>Frequency</MemberName>
        <Name>Frequency</Name>
        <Value>1</Value>
      </Expression>
      <Expression xsi:type="Divide">
        <Operand xsi:type="IntProperty">
          <Value>6</Value>
        </Operand>
        <PropertyMappings />
      </Expression>
      <Expression xsi:type="Combinator">
        <PropertyMappings />
        <Combinator xsi:type="q3:AudioPlayback">
          <q3:Frequency>44100</q3:Frequency>
        </Combinator>
      </Expression>
      <Expression xsi:type="Combinator">
        <PropertyMappings />
        <Combinator xsi:type="q2:ConvertScale">
          <q2:Depth>S16</q2:Depth>
          <q2:Scale>1000</q2:Scale>
          <q2:Shift>0</q2:Shift>
        </Combinator>
      </Expression>
      <Expression xsi:type="Source">
        <PropertyMappings />
        <Generator xsi:type="q1:DigitalInput">
          <q1:PortName>COM4</q1:PortName>
          <q1:Pin>12</q1:Pin>
        </Generator>
      </Expression>
      <Expression xsi:type="NestedWorkflow">
        <Name>CycleWaveforms</Name>
        <Workflow>
          <Nodes>
            <Expression xsi:type="Mod">
              <Operand xsi:type="IntProperty">
                <Value>4</Value>
              </Operand>
              <PropertyMappings />
            </Expression>
            <Expression xsi:type="Accumulate" />
            <Expression xsi:type="Add">
              <PropertyMappings />
            </Expression>
            <Expression xsi:type="Combinator">
              <PropertyMappings />
              <Combinator xsi:type="CombineLatest" />
            </Expression>
            <Expression xsi:type="Source">
              <PropertyMappings />
              <Generator xsi:type="IntProperty">
                <Value>0</Value>
              </Generator>
            </Expression>
            <Expression xsi:type="WorkflowOutput" />
            <Expression xsi:type="WorkflowInput">
              <Name>Source1</Name>
            </Expression>
          </Nodes>
          <Edges>
            <Edge>
              <From>0</From>
              <To>5</To>
              <Label>Source1</Label>
            </Edge>
            <Edge>
              <From>1</From>
              <To>0</To>
              <Label>Source1</Label>
            </Edge>
            <Edge>
              <From>2</From>
              <To>1</To>
              <Label>Source1</Label>
            </Edge>
            <Edge>
              <From>3</From>
              <To>2</To>
              <Label>Source1</Label>
            </Edge>
            <Edge>
              <From>4</From>
              <To>3</To>
              <Label>Source1</Label>
            </Edge>
            <Edge>
              <From>6</From>
              <To>3</To>
              <Label>Source2</Label>
            </Edge>
          </Edges>
        </Workflow>
        <PropertyMappings />
      </Expression>
      <Expression xsi:type="ExternalizedPropertyOfFunctionWaveformFunctionGenerator">
        <MemberName>Waveform</MemberName>
        <Name>Waveform</Name>
        <Value>Sine</Value>
      </Expression>
    </Nodes>
    <Edges>
      <Edge>
        <From>0</From>
        <To>3</To>
        <Label>Source1</Label>
      </Edge>
      <Edge>
        <From>1</From>
        <To>5</To>
        <Label>Source1</Label>
      </Edge>
      <Edge>
